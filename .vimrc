" Use VIM
set nocompatible

" plugin
source ~/.vim/.rcfiles/.vimrc.plugin
source ~/.vim/.rcfiles/.vimrc.plugconf

" basic settings
source ~/.vim/.rcfiles/.vimrc.editor

" color customize
source ~/.vim/.rcfiles/.vimrc.color

" filetype
source ~/.vim/.rcfiles/.vimrc.type

" project settings
source ~/.vim/.rcfiles/.vimrc.projects

" keymapping, custom command
source ~/.vim/.rcfiles/.vimrc.custom

" Other
source ~/.vim/.rcfiles/.vimrc.other

